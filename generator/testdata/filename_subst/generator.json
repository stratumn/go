{
  "name": "subst-tmpl",
  "version": "0.2.0",
  "description": "Subst test",
  "author": "Stratumn",
  "license": "Apache-2.0",
  "filename-substitutions": {
    "FOOBAR": "process",
    "XXX": "name"
  },
  "inputs": {
    "name": {
      "type": "string",
      "prompt": "Project name:",
      "default": "{{.dir}}",
      "format": ".+"
    },
    "process": {
      "type": "slice:string",
      "prompt": "List of process names:",
      "default": "{{.process}}",
      "format": "^[a-zA-Z].*$",
      "separator": ","
    }
  }
}
